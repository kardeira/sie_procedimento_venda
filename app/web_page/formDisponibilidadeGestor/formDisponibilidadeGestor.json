      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.oportunidadeVenda ){",
        "    if ($data.oportunidadeVenda.isGestorAvailable !== undefined) {",
        "        return {",
        "\t\t//map oportunidadeVenda variable to expected task contract input",
        "    \t\toportunidadeVendaInput: {",
        "    \t\t\tisGestorAvailable: $data.oportunidadeVenda.isGestorAvailable === 'Sim' ? true : false",
        "    \t\t}",
        "        }",
        "\t}",
        "\t        return {",
        "\t\t//map oportunidadeVenda variable to expected task contract input",
        "    \t\toportunidadeVendaInput: {",
        "    \t\t\tisGestorAvailable: null",
        "    \t\t}",
        "        }",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
